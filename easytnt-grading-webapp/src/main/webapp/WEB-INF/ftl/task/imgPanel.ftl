<#macro imgPanel>
<div class="img-panel">
 <div id="imgContainer" style="height: 100%;"></div>
 <div class="panel panel-success  img-panel-toolbox">
  <div class="panel-heading">
   <div class="pull-left">
    <i class=" glyphicon   icon-remove "></i>
   </div>
  </div>
  <div class="panel-body ">
   <ul>
    <li class="row" >
      <div class="col-md-12">
        <button type="button" class=" button   button-plain button-border button-square button-tiny" style="margin-bottom:8px;"><b class="left">←</b><b class="right">→</b></button>
      </div>
    </li>
    <li role="separator" class="divider"></li>
    <li class="row" >
      <div class="col-md-12">
        <button type="button" class=" button   button-plain button-border button-square button-tiny" ><b class="top">↑</b><b class="bottom">↓</b></button>
      </div>
    </li>
    <li role="separator" class="divider"></li>
    <li class="row" >
      <div class="col-md-12">
        <button type="button" class=" button   button-plain button-border button-square button-tiny" ><b class="left">←</b><b class="right">→</b><b class="top" style="top:3px;">↑</b><b class="bottom" style="top:5px;">↓</b></button>
      </div>
    </li>
    <li role="separator" class="divider"></li>
    <li class="row" >
      <div class="col-md-12">
        <button type="button" class=" button   button-plain button-border button-square button-tiny" > <b class="colorful"></b></button>
      </div>
    </li>
    <li role="separator" class="divider"></li>
    <li class="row" >
      <div class="col-md-12">
        <button type="button" class=" button   button-plain button-border button-square button-tiny" ><b style="color:#ff0000;">✔</b></button>
      </div>
    </li>
    <li role="separator" class="divider"></li>
    <li class="row" >
      <div class="col-md-12">
        <button type="button" class=" button   button-plain button-border button-square button-tiny" ><b style="color:#ff0000;">✘</b></button>
      </div>
    </li>
    <li role="separator" class="divider"></li>
    <li class="row" >
      <div class="col-md-12">
        <button type="button" class=" button   button-plain button-border button-square button-tiny" style="margin-bottom:8px;"><b style="color:#ff0000;">T</b></button>
      </div>
    </li>
    <li role="separator" class="divider"></li>
    <li class="row" >
      <div class="col-md-12">
        <button type="button" class=" button   button-plain button-border button-square button-tiny" ><i class="icon-stop" style="font-size:8px;    position: absolute;"></i></button>
      </div>
    </li>
    <li role="separator" class="divider"></li>
    <li class="row" > 
      <div class="col-md-12">   
        <div class="logv"></div>
      </div>
    </li>       
   </ul>
  </div>
 </div>
</div>
</#macro>